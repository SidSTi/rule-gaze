<div class="dashboard container">
  <div class="row">
    <div class="col">
      {{dropzone-container
        model=model
        onFileAdd=(action "uploadData")
        onClickDelete=(action "deleteFile")
        onClickSelect=(action "selectFile")
      }}
    </div>
  </div>
  <div class="row engage-visualization">
    <div class="col d-flex justify-content-center align-items-center">
      {{#link-to "visualize" class=(concat "btn btn-outline-info " (unless hasNecessarySelectedFiles "disabled"))}}
        Visualize
      {{/link-to}}
    </div>
  </div>
</div>